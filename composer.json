{
    "name": "husseinferas/laraimage",
    "license": "MIT",
    "description": "A Laravel package that adds a simple image functionality to any Laravel model",
    "keywords": ["laravel","media","image","laraimage","imageable"],
    "authors": [
        {
            "name": "Hussein Feras",
            "email": "husseinferas12@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "extra": {
        "laravel": {
            "providers": [
                "HusseinFeras\\Laraimage\\LaraimageServiceProvider"
            ]
        }
    },
    "autoload": {
        "psr-4": {
            "HusseinFeras\\Laraimage\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "HusseinFeras\\Laraimage\\Test\\": "tests/"
        }
    },
    "require": {
        "php": ">=7.2",
        "illuminate/database": "^6.0|^7.0|^8.0",
        "illuminate/support": "^6.0|^7.0|^8.0"
    },
    "require-dev": {
        "mockery/mockery": "^1.3",
        "orchestra/testbench": "^4.0|^5.0|^6.0"
    }
}
